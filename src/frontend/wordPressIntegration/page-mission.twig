    const nftaddress = '0x9BA0538A6755b06E45B63e9d6f1CaB1d86555Cd9'; // todo: replace with correct address and abi
    const tokenaddress = '0x08edb028770140c2AC870C93F75DF94b5AD5157A'; // todo: replace with correct address and abi
    const nftstakeraddress = '0xD8CE97D86AB774B80Cb30fBa9acF8Dd47541f364'; // todo: replace with correct address and abi
    let items = [];
    let accounts = [];
    let provider = null;
    let signer = null;
    let isStaked = false;
    let token = null;
    let nftstaker = null;
    let nft = null;

    const main = async function() {
      accounts = await window.ethereum.request({ method: 'eth_requestAccounts' });
      provider = new ethers.providers.Web3Provider(window.ethereum)
      signer = provider.getSigner()
      token = new ethers.Contract(tokenaddress, tokenabi, signer)
      nftstaker = new ethers.Contract(nftstakeraddress, nftstakerabi, signer)
      nft = new ethers.Contract(nftaddress, nftabi, signer)
      
      const noTokenFound = function() {
          console.log("No tokens found for this address");
      }

      const updateBalance = async function() {
        console.log("updateBalance...");
        try {
          const balance = await token.balanceOf(accounts[0]);
          console.log("balance: " + balance);
          $("#token-balance").text(balance);
        }
        catch(error) {
          console.log("updateBalance " + error)
          $("#token-balance").text("error");
        }
      }

      const updateIsTokenStaked = async function() {
        console.log("updateIsTokenStaked...");
        if (items.length > 0) {
          try {
            isStaked = await nftstaker.isTokenStaked(items[0]?.token_id);
            console.log("isStaked: " + isStaked);
            if (isStaked) {
              $("#button-stake").text("Unstake");
            } else {
              $("#button-stake").text("Stake");
            }
          }
          catch(error) {
            console.log("updateIsTokenStaked " + error)
            $("#button-stake").text("error");
          }
        }
        else {
          noTokenFound();
        }
      }

      const loadOpenSeaItems = async () => {
        console.log("loadOpenSeaItems...");
        const urlApi = 'https://testnets-api.opensea.io/api/v1' // testnet
        // const urlApi = 'https://api.opensea.io/api/v1' // mainnet
        const finalUrl = `${urlApi}/assets?owner=${accounts[0]}&asset_contract_address=${nftaddress}&format=json`
        console.log("Sending api call for items to " + finalUrl)

        items = await fetch(finalUrl)
        .then((res) => res.json())
        .then((res) => {
          return res.assets
        })
        .catch((e) => {
          console.error(e)
          console.error('Could not talk to OpenSea')
          return null
        })

        console.log("items loaded: ");
        console.log(items);

        if (items != null && items.length > 0) {
          $("#items-count").text(items?.length + " Items");
          $("#item-name").text(items[0]?.name);
          $("#item-description").text(items[0]?.description);
          document.getElementById("item-image").src = items[0]?.image_url;
        }
        else {
          noTokenFound();
        }
      }
      await loadOpenSeaItems();
      await updateIsTokenStaked();
      await updateBalance();
    }
    main()
    
    const stakeUnstakeFunction = async function() {
      if (items.length > 0) {
        console.log('stakeUnstakeFunction called ' + items[0]?.token_id)
        if (!isStaked) {
          console.log("Set approval for all");
          await(await nft.setApprovalForAll(nftstakeraddress, true)).wait()
          console.log("Stake nft...")
          await(await nftstaker.stake(items[0]?.token_id)).wait()
        } else {
          console.log("Unstake nft...")
          await(await nftstaker.unstake(items[0]?.token_id)).wait()
        }
      }
      else {
        noTokenFound();
      }
    }
	</script>

